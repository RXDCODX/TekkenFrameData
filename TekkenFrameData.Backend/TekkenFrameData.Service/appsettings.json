{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "docker_pg": "Server=tekken_db;Port=5432;Database=tekken_db;User Id=postgres;Password=postgres;",
    "local_pg": "Server=localhost;Port=5432;Database=tekken_frame_data;User Id=postgres;Password=postgres;"
  },
  "JwtSettings": {
    "SecretKey": "your-super-secret-key-with-at-least-32-characters-long",
    "Issuer": "TekkenFrameData",
    "Audience": "TekkenFrameData",
    "ExpiresInMinutes": 60
  },
  "OAuth": {
    "Twitch": {
      "ClientId": "your-twitch-client-id",
      "ClientSecret": "your-twitch-client-secret",
      "RedirectUri": "https://localhost:5000/api/v1/auth/twitch-callback"
    },
    "Google": {
      "ClientId": "your-google-client-id",
      "ClientSecret": "your-google-client-secret",
      "RedirectUri": "https://localhost:5000/api/v1/auth/google-callback"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "route1": {
        "ClusterId": "cluster1",
        "Match": {
          "Path": "{**catch-all}"
        }
      }
    },
    "Clusters": {
      "cluster1": {
        "Destinations": {
          "destination1": {
            "Address": "https://tekken_frontend:8060/"
          }
        }
      }
    }
  }
}
